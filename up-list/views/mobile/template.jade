extends layout

block content
  link(rel="stylesheet", href="/m/css/mdui.min.css")
  link(rel="stylesheet", href="/m/css/#{css}.css")
  - if (cssVendor)
    link(rel="stylesheet", href="/m/css/#{cssVendor}.css")
  script(src="/m/js/mdui.min.js")
  script(src="/javascripts/vue.min.js")
  script(src="/javascripts/axios.min.js")
  #app
  - if (!keepDrawer)
    script.
      mdui.JQ('body').removeClass('mdui-drawer-body-left');
  - if (jsVendor)
    script(src="/m/js/#{jsVendor}.js")
  - if (injectedLibs)
  - for (let lib of injectedLibs) {
      script(src="m/js/#{lib}.js")
  - }
  script(src="/m/js/#{js}.js")